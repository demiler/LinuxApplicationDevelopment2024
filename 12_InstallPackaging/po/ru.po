#: ../src/avg.c:19
#, c-format
msgid "Usage: avg [<options>]\n"
msgstr "Использование: avg [<опции>]\n"

#: ../src/avg.c:20
#, c-format
msgid "  Calculates average of your input. Very usefull!\n"
msgstr "  Считает среднее вашего ввода. Очень удобно!\n"

#: ../src/avg.c:22
#, c-format
msgid "Options:\n"
msgstr "Опции:\n"

#: ../src/avg.c:23
#, c-format
msgid "  -m                Output multiplication average instead\n"
msgstr "  -m                Выводить среднее умножения\n"

#: ../src/avg.c:24
#, c-format
msgid "  -h, --help        Show this message\n"
msgstr "  -h, --help         Вывести это сообщение\n"

#: ../src/avg.c:25
#, c-format
msgid "  -v, --version     Show programm version\n"
msgstr "  -v, --version      Показать версию программы\n"

#: ../src/avg.c:43
#, c-format
msgid "Version: %s\n"
msgstr "Версия: %s\n"

#: ../src/avg.c:48
#, c-format
msgid "Enter amount of numbers you wan't to enter: "
msgstr "Введите количество чисел, которые вы хотите ввести: "

#: ../src/avg.c:53
#, c-format
msgid "Error: too many numbers, I don't have enough memory for this\n"
msgstr "Ошибка: сликшом много чисел, у меня нет столько памяти\n"

#: ../src/avg.c:58
#, c-format
msgid "Enter numbers themselfs:\n"
msgstr "Введите сами числа\n"

#: ../src/avg.c:64
#, fuzzy, c-format
msgid "Here is your average mul: %lld\n"
msgstr "Вот ваше среднее произведение: %lld\n"

#: ../src/avg.c:67
#, c-format
msgid "Here is your average sum: %lld\n"
msgstr "Вот ваша средняя сумма: %lld\n"
